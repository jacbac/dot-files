[user]
    name = {your_name}
    email = {your_email}@gmail.com
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    pom = push origin master
    hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
    type = cat-file -t
    dump = cat-file -p
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow
    frag = magenta
    old = red
    new = green
[color "status"]
    added = green
    changed = red
    untracked = cyan
[push]
    default = simple
