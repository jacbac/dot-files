[user]
    name = {your_name}
    email = {your_email}@gmail.com
[core]
    repositoryformatversion = 0
    filemode = true
    autocrlf = input
[alias]
    co = checkout
    cm = commit -m
    st = status
    br = branch
    pom = push origin master
    hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
    type = cat-file -t
    dump = cat-file -p
    lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow
    frag = magenta
    old = red
    new = green
[color "status"]
    added = green
    changed = red
    untracked = cyan
[push]
    default = simple
